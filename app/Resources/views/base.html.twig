<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <meta name="description" content="{% block description %}{% trans %}application.description{% endtrans %}{% endblock %}">
        <meta name="author" content="{% block author %}{% trans %}application.author{% endtrans %}{% endblock %}">

        <title>{% block title %}{% trans %}application.shortname{% endtrans %}{% endblock %}</title>

        {% block stylesheets -%}
            {% include "::strongly-branded.html.twig" %}
        {% endblock %}

        <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ asset('apple-touch-icon-precompiled-144.png') }}">
        <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ asset('apple-touch-icon-precompiled-114.png') }}">
        <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ asset('apple-touch-icon-precompiled-72.png') }}">
        <link rel="apple-touch-icon-precomposed" sizes="57x57" href="{{ asset('apple-touch-icon-precompiled-57.png') }}">
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.png') }}" />

        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
          <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>
    <body {% block body_attrs %}{% endblock %}>
        {% block body %}{% endblock %}
        {% block javascripts %}
            {% javascripts '@jquery_js' '@jqueryui_js' '@bootstrap_js' '@dashboard_js' filter='?yui_js' combine=true %}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
    </body>
</html>
